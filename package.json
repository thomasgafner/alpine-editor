    "rollup": "^2.28.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "vuepress": "^1.6.0"
  },
  "dependencies": {
    "lodash-es": "^4.17.15",
    "prosemirror-commands": "^1.1.4",
    "prosemirror-keymap": "^1.1.4",
    "prosemirror-model": "^1.11.2",
